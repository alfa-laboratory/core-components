        "jest-image-snapshot": "4.2.0",
        "json": "10.0.0",
        "lerna": "3.20.2",
        "lint-staged": "10.0.8",
        "lodash.camelcase": "4.3.0",
        "lodash.kebabcase": "4.1.1",
        "lodash.upperfirst": "4.3.1",
        "mini-css-extract-plugin": "0.11.0",
