        "alfa-ui-primitives": "3.13.0",
        "classnames": "2.2.6",
        "compute-scroll-into-view": "1.0.13",
        "date-fns": "2.16.1",
        "downshift": "5.4.7",
        "element-closest": "3.0.2",
        "intersection-observer": "0.12.0",
        "libphonenumber-js": "1.8.4",
